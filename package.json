{
  "name": "luix-passport",
  "version": "1.0.0",
  "private": true,
  "description": "Luix Passport Package Management",
  "license": "UNLICENSED",
  "scripts": {
    "app:start": "./mvnw",
    "all:up": "docker compose -f docker/all.yml up --wait",
    "prod": "docker compose -f docker/passport-server.yml up --wait",
    "java:docker": "./mvnw -pl luix-passport-server,luix-passport-resource-server,luix-passport-client -ntp verify -DskipTests -Prelease jib:dockerBuild",
    "java:docker:arm64": "npm run java:docker -- -Djib-maven-plugin.architecture=arm64"
  }
}
